{
  "version": 2,
  "builds": [
    {
      "src": "api/index.go",
      "use": "@vercel/go"
    },
    {
      "src": "api/auth.go",
      "use": "@vercel/go"
    },
    {
      "src": "api/courses.go",
      "use": "@vercel/go"
    },
    {
      "src": "api/quizzes.go",
      "use": "@vercel/go"
    }
  ],
  "routes": [
    {
      "src": "/api/auth/(.*)",
      "dest": "/api/auth.go"
    },
    {
      "src": "/api/courses/(.*)",
      "dest": "/api/courses.go"
    },
    {
      "src": "/api/quizzes/(.*)",
      "dest": "/api/quizzes.go"
    },
    {
      "src": "/api/?",
      "dest": "/api/index.go"
    }
  ],
  "env": {
    "GO111MODULE": "on"
  }
}
