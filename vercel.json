{
  "version": 2,
  "buildCommand": "cd frontend && npm install && npm run build",
  "outputDirectory": "frontend/.next",
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/next"
    },
    {
      "src": "api/index.go",
      "use": "@vercel/go"
    },
    {
      "src": "api/auth.go",
      "use": "@vercel/go"
    },
    {
      "src": "api/courses.go",
      "use": "@vercel/go"
    },
    {
      "src": "api/quizzes.go",
      "use": "@vercel/go"
    }
  ],
  "routes": [
    {
      "src": "/api/auth/(.*)",
      "dest": "/api/auth.go"
    },
    {
      "src": "/api/courses/(.*)",
      "dest": "/api/courses.go"
    },
    {
      "src": "/api/quizzes/(.*)",
      "dest": "/api/quizzes.go"
    },
    {
      "src": "/api/?",
      "dest": "/api/index.go"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/$1"
    }
  ],
  "env": {
    "GO111MODULE": "on"
  }
}
